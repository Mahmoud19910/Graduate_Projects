<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#E32C83"/>
            <corners android:radius="@dimen/_30sdp"/>
            <stroke android:width="3dp" android:color="#33000000"/>


        </shape>
    </item>

    <item
        android:left="0dp"
        android:right="0dp"
        android:top="3dp"
        android:bottom="3dp">
        <shape android:shape="rectangle">
            <solid android:color="#ffffff"/>
            <stroke android:color="#707070"/>
            <corners android:radius="@dimen/_30sdp"/>
        </shape>
    </item>
</layer-list>